</head>

<body>
    
   <div class="main">
       <div class="main__left">
           <div class="aler" >
               
           </div>
            <div class="main__videos">
               
                <div id ="video-grid">
                    
                </div>
                <!-- <div style="width: 600px; height: 550px;" id="wt-container"></div> -->
            <!-- <script src="https://www.whiteboard.team/dist/api.js"></script>
            <script type="text/javascript">
                var wt = new api.WhiteboardTeam('#wt-container', {
                clientId: '796d0ee71974784eedfc4720f35502bc',
                boardCode: ``
                });
            </script> -->
            </div> 
            <div class="main__controls">
                <div class="main__controls__block">
                    <div onclick="muteUnmute()" class="main__controls__button main__mute__button"> 
                        <i class="fas fa-microphone"></i>
                        <span>Mute</span>
                    </div>
                    <!-- <button id="start">
                        Start Recording
                      </button>
                      <button id="stop" disabled>
                        Stop Recording
                      </button> -->
                      
                    <!--<button id="start">Capture</button>
                    <button id="stop">Stop Capture</button> -->
                    <div onclick="playStop()" class="main__controls__button main__video__button">
                        <i class="fas fa-video"></i>
                        <span>Stop video</span>
                    </div>
                </div>
                <div class="main__controls__block ">
                    <div onclick="startCapture()" class="main__controls__button main__share__button">
                        <i class="fas fa-shield-alt"></i>
                        <span>Share Screen</span>
                    </div>
                    <div onclick="showParticipants()" class="main__controls__button main__parti__button">
                        <i class="fas fa-user-friends"></i>
                        <span>Participants</span>
                    </div>
                    <div onClick ="showChat()" class="main__controls__button main__chat__button">
                        <i class="fas fa-comment-alt"></i>
                        <span>Show Chats</span>
                    </div>
                     </div>
                <div class="main__controls__block">
                    <div onclick="ChatRoom()" class="main__controls__button">
                        <span class="leave_meeting">Leave Video Meet</span>
                    </div>
                </div>
            </div> 
       </div>
       <div class="main__header__in__chat" style="display: none;">
        <div class="meeetingName" style="left: 10%;padding-top: 10%;"><h2>Meeting-name</h2></div>
        <div onClick ="joinVideo()" class="main__controls__button main__join___video">
            <i class="fas fa-video"></i>
            <span>Join Video-Meet</span>
        </div>
        <div onClick ="leaveRoom()" class="main__controls__button main__leave__room">
            <i class="fas fa-phone-slash" style="color: red;"></i>
            <span>Leave Room</span>
        </div>
       </div>
       <div class="main__right">
           
            <div class="main__header">
                <h5>Chat</h5>
            </div>
            <div class="main__chat__window">
                <ul class="messages">

                </ul>
            </div>
            <div class="main__message__container">
                <input id="chat__message" type="text" placeholder="Type message here ....">
            </div>
       </div>
       <div class="main__right__1">
        <div class="main__header__1">
            <h6>Participants</h6>
        </div>
        <div class="main__chat__window__1">
            <!-- <ul class="messages"> -->
            <ul class="participants__list">

            </ul>
        </div>
        <!-- <div class="main__message__container">
            <input id="chat__message" type="text" placeholder="Type message here ....">
        </div> -->
   </div>
